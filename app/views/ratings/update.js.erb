$('#star').raty({
  readOnly: true,
  score: "#{ @vino.average_rating }",
  path: "../assets/"
});

$('#user_rating').raty({
  score: "#{ @rating.valoracion }",
  path: "../assets/",
  click: function(score, evt) {
    $.ajax({
      url: "/ratings/#{ @rating.id }",
      type: "PATCH",
      data: { score: score }
    }).done(function(data){});
  }
});
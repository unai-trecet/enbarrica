$('#vino_rating').raty({
  readOnly: true,
  score: "<%= @vino.average_rating %>",
  path: "../assets/"
});

$('#valoracion_usuario').raty({
  readOnly: true,
  score: "<%= @rating.valoracion %>",
  path: '../assets/'
});   

$('#boton_cambiar_valoracion').raty({
  score: "<%= @rating.valoracion %>",
  path: "../assets/",
  
  click: function(score, evt) {
    $.ajax({
      url: "/ratings/#{ @rating.id }",
      type: "PATCH",
      data: { valoracion: score }
    }).done(function(data){});
  }
});

